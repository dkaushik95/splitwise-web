{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx,js,jsx}"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "**/__tests__/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "eslint",
    "eslint-config-next"
  ],
  "ignoreBinaries": [],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "unlisted": "error",
    "exports": "error",
    "nsExports": "error",
    "nsTypes": "error",
    "types": "error",
    "enumMembers": "error",
    "classMembers": "error",
    "duplicates": "error"
  }
}